service ppd /system/bin/mm-pp-daemon
    class late_start
    user system
    socket pps stream 0660 system system
    group system graphics

on property:init.svc.surfaceflinger=stopped
    stop ppd

# start netdump for debugging
service netdump /system/xbin/tcpdump -i any -C 20 -p -s 0 -w /sdcard/netdump -Z root -W 10
    user root
    disabled

# start netdump-without-loop for debugging
service netdump-woloop /system/xbin/tcpdump -i any -C 20 -p -s 0 -w /sdcard/netdump -Z root
    user root
    disabled

on property:persist.service.netdump.enable=2
    stop netdump
    start netdump-woloop

on property:persist.service.netdump.enable=1
    stop netdump-woloop
    start netdump

on property:persist.service.netdump.enable=0
    stop netdump
    stop netdump-woloop

service ml_daemon /system/bin/ml_daemon
    class core
    user root
    group root

service seald /system/bin/seald
    class late_start
    user root
    group system

service energy-awareness /system/bin/energy-awareness
    class main
    user root
    group system
    oneshot

service dts_configurator /system/bin/dts_configurator
