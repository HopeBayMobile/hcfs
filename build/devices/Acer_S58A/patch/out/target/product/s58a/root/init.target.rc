# bugreport is triggered by holding down volume down, volume up and power
service bugreport /system/bin/dumpstate -d -p -B \
        -o /data/data/com.android.shell/files/bugreports/bugreport
    class main
    disabled
    oneshot
    keycodes 114 115 116

# For acer flip
on property:persist.sys.lidhall.enable=1
    write /sys/class/switch/hall/enable 1
on property:persist.sys.lidhall.enable=0
    write /sys/class/switch/hall/enable 0

#Acer log service
service acer_log /system/bin/alog
    user root
    group system
    oneshot

on property:persist.service.alog.enable=1
    start acer_log

on property:persist.service.alog.enable=0
    stop acer_log

# dinfo service
service acer_dinfo_init /system/bin/acer_dinfo_init
