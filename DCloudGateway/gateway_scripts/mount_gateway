sudo mount.s3ql --allow-other --nfs --profile --metadata-upload-interval 3600 --cachesize 20480000 --max-cache-entries 250000 swift://$1:8080/$2 /mnt/cloudgwfiles
sleep 10
sudo /etc/init.d/iscsitarget start
sudo /etc/init.d/portmap start
sudo /etc/init.d/nfs-kernel-server start
sudo /etc/init.d/smbd start
sudo /etc/init.d/nmbd start
sleep 10
sudo /etc/init.d/iscsitarget restart

